<h1>Listing leaves</h1>

<table>
  <tr>
    <th>Employee</th>
    <th>Els</th>
    <th>Nels</th>
    <th>Lops</th>
    <th>Compoffs</th>
    <th>Is special</th>
    <th>Createdby</th>
    <th>Modifiedby</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @leaves.each do |leafe| %>
  <tr>
    <td><%= leafe.employee_id %></td>
    <td><%= leafe.els %></td>
    <td><%= leafe.nels %></td>
    <td><%= leafe.lops %></td>
    <td><%= leafe.compoffs %></td>
    <td><%= leafe.is_special %></td>
    <td><%= leafe.createdby %></td>
    <td><%= leafe.modifiedby %></td>
    <td><%= link_to 'Show', leafe %></td>
    <td><%= link_to 'Edit', edit_leafe_path(leafe) %></td>
    <td><%= link_to 'Destroy', leafe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

